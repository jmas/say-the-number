{
  "name": "Numbers to Words",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Convert Numbers to Words.",
  "author": "Maslakov Alexander <jmas.ukraine@gmail.com>",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
